{
    "name": "tsuibami",
    "version": "0.1",
    "manifest_version": 2,

    "description": ":esa: with chrome",

    "permissions": [
        "tabs", "<all_urls>",
        "storage"
    ],

    "options_page": "options.html",

    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+I",
                "mac": "Command+I"
            }
        }
    },

    "browser_action": {
        "default_icon": "icon19.png",
        "default_title": "tsuibami",
        "default_popup": "index.html"
    }
}
